<mapping>
	<strength>normal</strength>
	<source>
		<path>name</path>
	</source>
	<source>
		<path>givenName</path>
	</source>
	<source>
		<path>familyName</path>
	</source>
	<expression>
		<script>
			<code>
				givenName + ' ' + familyName
			</code>
		</script>
	</expression>
	<target>
		<path>fullName</path>
	</target>
	<condition>
		<script>
			<code>
				basic.stringify(name) != 'jack'
			</code>
		</script>
	</condition>
</mapping>